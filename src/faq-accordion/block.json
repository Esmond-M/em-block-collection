{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "em-block-faq-accordion/em-block-faq-accordion",
	"version": "0.1.0",
	"title": "EM FAQ Accordion",
	"category": "widgets",
	"description": "Display FAQs in an accordion block.",
	"supports": {
		"html": false
	},
	"attributes": {
		"faqs": {
			"type": "array",
			"default": []
		}
	},
	"textdomain": "em-block-faq-accordion",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
